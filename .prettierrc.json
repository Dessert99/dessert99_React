{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "quoteProps": "as-needed",
  "printWidth": 100,
  "proseWrap": "never",
  "tabWidth": 2,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss"],
  "jsxSingleQuote": true,
  "singleAttributePerLine": true,
  "bracketSameLine": true
}
